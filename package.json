{
	"name": "add-function-return-types",
	"version": "0.0.0",
	"description": "A CLI tool to add explicit return types to TypeScript functions",
	"files": [
		"dist/"
	],
	"main": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"keywords": [
		"typescript",
		"cli",
		"tool",
		"return",
		"types",
		"explicit-function-return-type"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brandhaug/add-function-return-types.git"
	},
	"bugs": {
		"url": "https://github.com/brandhaug/add-function-return-types/issues"
	},
	"homepage": "https://github.com/brandhaug/add-function-return-types#readme",
	"type": "module",
	"bin": {
		"add-function-return-types": "dist/index.js"
	},
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"lint": "biome lint",
		"check-fix": "biome check --fix",
		"typecheck": "tsc",
		"prepare": "husky",
		"test": "vitest",
		"validate": "npm run lint && npm run typecheck && npm run test"
	},
	"dependencies": {
		"commander": "14.0.0",
		"fast-glob": "3.3.3",
		"ts-morph": "26.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.4",
		"@commitlint/cli": "19.8.1",
		"@commitlint/config-conventional": "19.8.1",
		"execa": "9.6.0",
		"husky": "9.1.7",
		"lint-staged": "16.1.5",

		"tsc-files": "1.1.4",
		"tsx": "4.20.3",
		"typescript": "5.9.2",
		"vitest": "3.2.4",
		"@semantic-release/changelog": "6.0.3",
		"semantic-release": "24.2.7"
	},
	"author": "Martin Brandhaug",
	"license": "MIT",
	"lint-staged": {
		"*.{ts,json}": [
			"biome check --write"
		],
		"*.ts": [
			"tsc-files"
		]
	},
	"engines": {
		"node": ">=20"
	}
}
