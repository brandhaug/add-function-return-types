{
	"name": "add-function-return-types",
	"version": "1.1.6",
	"description": "A CLI tool to add explicit return types to TypeScript functions",
	"files": [
		"dist/"
	],
	"main": "./dist/index.js",
	"keywords": [
		"typescript",
		"cli",
		"tool",
		"return",
		"types",
		"explicit-function-return-type"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/brandhaug/add-function-return-types.git"
	},
	"bugs": {
		"url": "https://github.com/brandhaug/add-function-return-types/issues"
	},
	"homepage": "https://github.com/brandhaug/add-function-return-types#readme",
	"type": "module",
	"bin": {
		"add-function-return-types": "dist/index.js"
	},
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"format": "biome format --write",
		"lint": "biome check --write",
		"typecheck": "tsc --noEmit",
		"prepare": "husky",
		"test": "vitest"
	},
	"dependencies": {
		"commander": "12.1.0",
		"fast-glob": "3.3.2",
		"p-limit": "6.1.0",
		"ts-morph": "24.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@commitlint/cli": "19.3.0",
		"@commitlint/config-conventional": "19.2.2",
		"@types/cli-progress": "3.11.6",
		"@types/fs-extra": "11.0.4",
		"execa": "9.4.1",
		"fs-extra": "11.2.0",
		"husky": "9.1.4",
		"lint-staged": "15.2.7",
		"tsx": "4.19.1",
		"typescript": "5.5.4",
		"vitest": "2.1.3"
	},
	"author": "Martin Brandhaug",
	"license": "MIT",
	"lint-staged": {
		"*.{js,ts}": [
			"biome format",
			"biome check"
		]
	}
}
